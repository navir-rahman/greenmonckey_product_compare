tz\"><\/td>\n       <\/tr>\n       <tr>\n           <td style=\"text-align: left;\">Transparenz<\/td>\n           <td id=\"transparenz-qr\">⭐️⭐️⭐️⭐️<\/td>\n           <td id=\"transparenz\"><\/td>\n       <\/tr>\n       <tr>\n           <td style=\"text-align: left;\">Kratzresistenz<\/td>\n           <td id=\"kratzresistenz-qr\">⭐️⭐️⭐️⭐️⭐️⭐️<\/td>\n           <td id=\"kratzresistenz\"><\/td>\n       <\/tr>\n       <tr>\n           <td style=\"text-align: left;\">Fingerabdruckresistenz<\/td>\n           <td id=\"fingerabdruckresistenz-qr\">⭐️⭐️⭐️⭐️<\/td>\n           <td id=\"fingerabdruckresistenz\"><\/td>\n       <\/tr>\n       <tr>\n           <td style=\"text-align: left;\">Glas Haptik<\/td>\n           <td id=\"haptik-qr\">⭐️⭐️⭐️⭐️<\/td>\n           <td id=\"haptik\"><\/td>\n       <\/tr>\n       <tr>\n           <td style=\"text-align: left;\">Selbstheilend<\/td>\n           <td id=\"selbstheilend-qr\">⭐️⭐️⭐️⭐️⭐️⭐️<\/td>\n           <td id=\"selbstheilend\"><\/td>\n       <\/tr>\n   <\/tbody>\n<\/table>\n\n<script>\n\/\/ Eigenschaften der Panzerfolien\nvar folien = {\n    crystalclear: {\n        name: \"CrystalClear\",\n        aufprallschutz: \"⭐️⭐️⭐️⭐️⭐️\",\n        transparenz: \"⭐️⭐️⭐️⭐️⭐️⭐️\",\n        kratzresistenz: \"⭐️⭐️⭐️⭐️\",\n        fingerabdruckresistenz: \"⭐️⭐️⭐️⭐️\",\n        haptik: \"⭐️⭐️⭐️⭐️⭐️\",\n        selbstheilend: \"⭐️⭐️⭐️\"\n    },\n    privacypro: {\n        name: \"PrivacyPro\",\n        aufprallschutz: \"⭐️⭐️⭐️⭐️⭐️\",\n        transparenz: \"⭐️⭐️⭐️⭐️\",\n        kratzresistenz: \"⭐️⭐️⭐️⭐️⭐️\",\n        fingerabdruckresistenz: \"⭐️⭐️⭐️⭐️⭐️⭐️\",\n        haptik: \"⭐️\",\n        selbstheilend: \"⭐️⭐️⭐️\"\n    },\n    eyecare: {\n        name: \"EyeCare\",\n        aufprallschutz: \"⭐️⭐️⭐️⭐️⭐️\",\n        transparenz: \"⭐️⭐️⭐️⭐️⭐️\",\n        kratzresistenz: \"⭐️⭐️⭐️⭐️\",\n        fingerabdruckresistenz: \"⭐️⭐️⭐️⭐️\",\n        haptik: \"⭐️⭐️⭐️⭐️\",\n        selbstheilend: \"⭐️⭐️⭐️\"\n    },\n    paperfeel: {\n        name: \"PaperFeel\",\n        aufprallschutz: \"⭐️⭐️⭐️⭐️⭐️\",\n        transparenz: \"⭐️⭐️⭐️⭐️\",\n        kratzresistenz: \"⭐️⭐️⭐️⭐️⭐️\",\n        fingerabdruckresistenz: \"⭐️⭐️⭐️⭐️⭐️⭐️\",\n        haptik: \"⭐️\",\n        selbstheilend: \"⭐️⭐️⭐️\"\n    },\n    hybridpro: {\n        name: \"HybridPro\",\n        aufprallschutz: \"⭐️⭐️⭐️⭐️⭐️\",\n        transparenz: \"⭐️⭐️⭐️⭐️⭐️⭐️\",\n        kratzresistenz: \"⭐️⭐️⭐️⭐️\",\n        fingerabdruckresistenz: \"⭐️⭐️⭐️\",\n        haptik: \"⭐️⭐️⭐️⭐️⭐️⭐️\",\n        selbstheilend: \"⭐️⭐️⭐️\"\n    }\n};\n\n\/\/ Funktion zum Aktualisieren der Tabelle\nfunction updateTable() {\n    var auswahl = document.getElementById(\"folien-auswahl\").value;\n    document.getElementById(\"aufprallschutz\").innerHTML = folien[auswahl].aufprallschutz;\n    document.getElementById(\"transparenz\").innerHTML = folien[auswahl].transparenz;\n    document.getElementById(\"kratzresistenz\").innerHTML = folien[auswahl].kratzresistenz;\n    document.getElementById(\"fingerabdruckresistenz\").innerHTML = folien[auswahl].fingerabdruckresistenz;\n    document.getElementById(\"haptik\").innerHTML = folien[auswahl].haptik;\n    document.getElementById(\"selbstheilend\").innerHTML = folien[auswahl].selbstheilend;\n    document.getElementById(\"aufprallschutz-qr\").innerHTML = folien[\"quickrepair\"].aufprallschutz;\n    document.getElementById(\"transparenz-qr\").innerHTML = folien[\"quickrepair\"].transparenz;\n    document.getElementById(\"kratzresistenz-qr\").innerHTML = folien[\"quickrepair\"].kratzresistenz;\n    document.getElementById(\"fingerabdruckresistenz-qr\").innerHTML = folien[\"quickrepair\"].fingerabdruckresistenz;\n    document.getElementById(\"haptik-qr\").innerHTML = folien[\"quickrepair\"].haptik;\n    document.getElementById(\"selbstheilend-qr\").innerHTML = folien[\"quickrepair\"].selbstheilend;\n}\n\n\/\/ Standardauswahl bei Seitenaufruf\nupdateTable();\n<\/script>","background_type":"full_width","background":"rgba(0,0,0,0)","text_color":"rgba(0,0,0,0)","custom_css":"table {\n  border-collapse: separate;\n  border-spacing: 0;\n  margin: 10px;\n  border-radius: 10px;\n  overflow: hidden;\n}\n\nth:first-child,\ntd:first-child {\n  border-top-left-radius: 10px;\n  border-bottom-left-radius: 0;\n}\n\nth:last-child,\ntd:last-child {\n  border-top-right-radius: 10px;\n  border-bottom-right-radius: 0px;\n}\n\ntr:first-child td:first-child,\ntr:first-child th:first-child {\n  border-top-left-radius: 0px;\n}\n\ntr:first-child td:last-child,\ntr:first-child th:last-child {\n  border-top-right-radius: 0px;\n}\n\ntr:last-child td:first-child,\ntr:last-child th:first-child {\n  border-bottom-left-radius: 10px;\n}\n\ntr:last-child td:last-child,\ntr:last-child th:last-child {\n  border-bottom-right-radius: 10px;\n}","mobile_custom_css":""}}},"order":["main","62b9c364-09b2-4d9c-89b7-d8ca2c0443f5","8fb1011f-5cce-481b-a7b7-5ebc0bcefc1a","79a10998-17d3-4282-a88b-1238b1025934","d45e4417-1a62-4f89-b82f-f794a6cf1261","de77243a-dd58-431e-95ea-048a08c1ddfc","34f5cca5-28df-4565-b3bc-43270e5f59c2","0b57f8fe-a247-4c71-ae7c-307c0864d837","1654028285738d53d0","16540287657eccca14","6d233b25-1084-4d0d-b446-d6ae8c0fa707"]}